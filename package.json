{
  "name": "webapis",
  "private": "true",
  "namespace": "@arcstack",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/ArcStackLab/webapis.git"
  },
  "packageManager": "pnpm@9.2.0",
  "description": "A Comprehensive Integration of All Web Standard APIs",
  "scripts": {
    "lint": "prettier . --check && eslint",
    "format": "prettier . --write",
    "uninstall-husky": "pnpm uninstall husky --no-save && git config --unset core.hooksPath && pnpm dlx rimraf .husky"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": [
      "eslint --fix --ext .js,.jsx,.ts,.tsx"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@nx/eslint": "19.5.7",
    "esbuild": "0.23.0",
    "eslint": "^9.9.0",
    "globals": "^15.9.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.7",
    "nx": "19.5.7",
    "prettier": "^3.3.3",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.0.1"
  },
  "keywords": [
    "web",
    "browser",
    "api"
  ],
  "author": "factman",
  "license": "MIT"
}
